{
  "name": "rcrd",
  "version": "1.0.0",
  "description": "A service to keep track of the nouns in your life",
  "main": "browser/index.js",
  "scripts": {
    "start": "node server/index.js",
    "lint": "standard",
    "pretest": "standard",
    "test": "ava",
    "build": "browserify -t reactify browser/index.js > static/index.js",
    "watch": "watchify -t reactify browser/index.js -o static/index.js",
    "predeploy": "npm test",
    "deploy": "node scripts/deploy/lambda.js && npm run build && node scripts/deploy/app.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jeffcarp/rcrd-dynamo.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jeffcarp/rcrd-dynamo/issues"
  },
  "homepage": "https://github.com/jeffcarp/rcrd-dynamo#readme",
  "devDependencies": {
    "adm-zip": "^0.4.7",
    "ava": "^0.14.0",
    "aws-sdk": "^2.2.33",
    "babel-cli": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "body-parser": "^1.15.0",
    "browser-request": "^0.3.3",
    "browserify": "^13.0.0",
    "canvas-dpi-scaler": "0.0.3",
    "classnames": "^2.2.3",
    "cors": "^2.7.1",
    "dynamodb-doc": "^1.0.0",
    "envify": "^3.4.0",
    "event-emitter": "^0.3.4",
    "express": "^4.13.4",
    "history": "^1.17.0",
    "moment": "^2.11.1",
    "moment-timezone": "^0.5.0",
    "pg": "^4.5.1",
    "pluralize": "^1.2.1",
    "proxyquire": "^1.7.3",
    "random-words": "0.0.1",
    "react": "^0.14.6",
    "react-dom": "^0.14.6",
    "react-router-component": "^0.29.0",
    "reactify": "^1.1.1",
    "request": "^2.69.0",
    "sinon": "^1.17.2",
    "standard": "^6.0.8",
    "store": "^1.3.20",
    "tap-spec": "^4.1.1",
    "tape": "^4.4.0",
    "test-lambda": "^1.0.0",
    "watchify": "^3.7.0"
